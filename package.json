{
  "name": "2026-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:worker": "wrangler dev",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler deploy",
    "test": "vitest run",
    "test:browser": "vitest run --config vitest.browser.config.ts",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "db:generate": "drizzle-kit generate && tsx scripts/convert-migrations.ts",
    "db:migrate": "wrangler d1 migrations apply 2026-tracker-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply 2026-tracker-db --remote",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "drizzle-orm": "^1.0.0-beta.6-7aaf14b",
    "hono": "^4.11.3",
    "pinia": "^3.0.4",
    "valibot": "^1.2.0",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251229.0",
    "@hono/valibot-validator": "^0.6.1",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "@vue/tsconfig": "^0.8.1",
    "drizzle-kit": "^1.0.0-beta.6-7aaf14b",
    "happy-dom": "^20.0.11",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.3.0",
    "vitest": "^4.0.16",
    "vitest-browser-vue": "^2.0.1",
    "vue-tsc": "^3.2.1",
    "wrangler": "^4.54.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.3.0"
  }
}
