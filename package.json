{
  "name": "2026-tracker",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:worker": "wrangler dev --port 8787",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "deploy": "pnpm run build && wrangler deploy",
    "lint": "oxlint --type-aware",
    "lint:fix": "oxlint --type-aware --fix",
    "test": "vitest run",
    "test:browser": "vitest run --config vitest.browser.config.ts",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "db:generate": "drizzle-kit generate && tsx scripts/convert-migrations.ts",
    "db:migrate": "wrangler d1 migrations apply 2026-tracker-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply 2026-tracker-db --remote",
    "db:studio": "drizzle-kit studio",
    "prepare": "husky",
    "update:all": "taze -I -f -l"
  },
  "dependencies": {
    "@fontsource-variable/inter": "5.2.8",
    "@iconify/vue": "5.0.0",
    "drizzle-orm": "1.0.0-beta.9-e89174b",
    "hono": "4.11.7",
    "ky": "1.14.2",
    "pinia": "3.0.4",
    "valibot": "1.2.0",
    "vue": "3.5.27",
    "vue-i18n": "11.2.8",
    "vue-router": "4.6.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20260122.0",
    "@hono/valibot-validator": "0.6.1",
    "@types/node": "25.0.10",
    "@vitejs/plugin-vue": "6.0.3",
    "@vitest/browser-playwright": "4.0.18",
    "@vitest/ui": "4.0.18",
    "@vue/tsconfig": "0.8.1",
    "drizzle-kit": "1.0.0-beta.12-a5629fb",
    "eslint-config-greenpie": "15.0.1-13",
    "happy-dom": "20.3.4",
    "husky": "9.1.7",
    "msw": "2.12.7",
    "oxlint": "1.41.0",
    "oxlint-tsgolint": "0.11.1",
    "taze": "19.9.2",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite": "8.0.0-beta.9",
    "vite-plugin-pwa": "1.2.0",
    "vitest": "4.0.18",
    "vitest-browser-vue": "2.0.2",
    "vue-tsc": "3.2.2",
    "workbox-window": "7.4.0",
    "wrangler": "4.60.0"
  },
  "packageManager": "pnpm@10.28.1",
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
